<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LinkPreview">
        <attr name="url" format="string"/>
        <attr name="previewStyle" format="enum">
            <enum name="simple" value="0"/>
            <enum name="banner" value="1"/>
            <enum name="strip" value="2"/>
            <enum name="details" value="3"/>
            <enum name="large" value="4"/>
        </attr>
        <attr name="enableDefaultClick" format="boolean"/>
        <attr name="borderRadius" format="dimension"/>
        <attr name="shadow" format="dimension"/>
        <attr name="backgroundColor" format="color" />
        <attr name="titleColor" format="color" />
        <attr name="descriptionColor" format="color" />
        <attr name="linkColor" format="color" />
        <attr name="originalLinkColor" format="color" />
    </declare-styleable>
</resources>